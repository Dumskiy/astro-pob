---
import Layout from "../layouts/Layout.astro";
import Section1 from "../components/pages/index/Section1.astro";
import Section2 from "../components/pages/index/Section2.astro";
import Section3 from "../components/pages/index/Section3.astro";
import Section6 from "../components/pages/index/Section6.astro";
import Cards from "../components/pages/index/Cards.astro";

import data from "../components/pages/index/index.json";
const data1 = data.page[0];
// const data2 = data.page[1];
const data3 = data.page[2];
const data4 = data.page[3];
const data6 = data.page[4];
const data7 = data.page[5];
---

<Layout>
  <Section1 data={data1} />
  <Section2 />
  <Section3 data={data3} />
  <Cards data={data4} />
  <Section6 data={data6} />
  <Cards data={data7} />
</Layout>

<style lang="scss"></style>
