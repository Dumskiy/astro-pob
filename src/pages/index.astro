import Section2 from "../components/pages/index/Section2.astro";
import Section3 from "../components/pages/index/Section3.astro";
import Cards from "../components/pages/index/Cards.astro";

import data from "../components/pages/index/index.json";
const data4 = data.page[0];
const data7 = data.page[1];
---

